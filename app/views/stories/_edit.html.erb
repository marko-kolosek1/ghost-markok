<%= form_with(model: @story, url: user_story_path(@story)) do |f| %>
  <p>
    <%= f.label :title, "Title" %><br/>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :tag_list, "Tags" %><br/>
    <%= f.text_field :tag_list %>
  </p>  
  <p>
    <%= f.label :content, "Content" %><br/>
    <%= f.text_area :content %>
  </p>
  <p>
    <%= f.submit 'Submit' %>
  </p>
<% end %>
